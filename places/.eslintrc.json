{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"], // Ensures commas after each item in multiline lists
    "comma-style": ["error", "last"], // Enforces commas to be placed at the end of each line
    "no-multi-spaces": ["error"] // Optional: Enforces single spaces between properties
  }
}
