{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "no-multi-spaces": [
      "error"
    ],
    "no-unused-vars": "error",
    "import/no-unresolved": "error"
  }
}